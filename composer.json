{
    "name": "kenny1911/doctrine-inherit-annotations",
    "description": "Support inherit doctrine annotations of parent class",
    "type": "library",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.1 || ^8.0",
        "doctrine/annotations": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kenny1911",
            "email": "o-muzyka@mail.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kenny1911\\DoctrineInheritAnnotations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kenny1911\\DoctrineInheritAnnotations\\Tests\\": "tests"
        }
    }
}
